<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Compile" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.default.props" />
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.props" />
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets" />
  <PropertyGroup>
    <ConfigurationType>StaticLibrary</ConfigurationType>
    <ShowAllFiles>false</ShowAllFiles>
    <LibraryName>Cavity.TaskManagement.EventLog.Messages.lib</LibraryName>
  </PropertyGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <BuildLog>
      <Path>$(MSBuildProjectName).log</Path>
    </BuildLog>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
    <BuildLog>
      <Path>$(MSBuildProjectName).log</Path>
    </BuildLog>
  </ItemDefinitionGroup>
  <ItemGroup>
    <ProjectConfiguration Include="Debug|Win32">
      <Configuration>Debug</Configuration>
      <Platform>Win32</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Release|Win32">
      <Configuration>Release</Configuration>
      <Platform>Win32</Platform>
    </ProjectConfiguration>
  </ItemGroup>
  <ItemGroup>
    <None Include="Messages.mc" />
  </ItemGroup>
  <Target Name="Compile" Condition=" '$(TargetFrameworkVersion)' == 'v4.0' or '$(TargetFrameworkVersion)' == '' ">
    <MakeDir Condition=" !Exists('$(ProjectDir)obj') "
             Directories="$(ProjectDir)obj" />
    <Message Text="FrameworkSdkDir = $(FrameworkSdkDir)" />
    <Exec Command="&quot;$(FrameworkSdkDir)bin\MC&quot; -u -U &quot;$(ProjectDir)Messages.mc&quot;"
          WorkingDirectory="$(ProjectDir)obj" />
    <Exec Command="&quot;$(FrameworkSdkDir)bin\RC&quot; -r &quot;$(ProjectDir)obj\Messages.rc&quot;"
          WorkingDirectory="$(ProjectDir)obj" />
    <Message Text="VCINSTALLDIR = $(VCINSTALLDIR)" />
    <Exec Command="&quot;$(VCINSTALLDIR)bin\LINK&quot; /VERBOSE -dll -noentry -machine:X86 -out:&quot;$(ProjectDir)obj\$(LibraryName)&quot; Messages.res"
          WorkingDirectory="$(ProjectDir)obj" />
    <Copy SourceFiles="$(ProjectDir)obj\$(LibraryName)"
          DestinationFolder="$(ProjectDir)bin"
          OverwriteReadOnlyFiles="true" />
  </Target>
  <Target Name="Clean Folders" AfterTargets="Clean">
    <RemoveDir Condition="Exists('$(ProjectDir)bin')"
               Directories="$(ProjectDir)bin"
               ContinueOnError="true" />
    <RemoveDir Condition="Exists('$(ProjectDir)obj')"
               Directories="$(ProjectDir)obj"
               ContinueOnError="true" />
  </Target>
</Project>